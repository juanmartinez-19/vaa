<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VanAlAeropuerto"
        tools:targetApi="31">
        <activity
            android:name=".activities.LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.VanAlAeropuerto" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.EmpresaActivity"
            android:exported="false"
            android:label="@string/title_activity_empresa"
            android:theme="@style/Theme.VanAlAeropuerto"/>

        <activity
            android:name=".activities.HomeActivity"
            android:exported="false"
            android:theme="@style/Theme.VanAlAeropuerto" />
    </application>

</manifest>