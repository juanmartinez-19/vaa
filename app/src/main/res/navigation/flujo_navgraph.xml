<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/flujo_navgraph"
    app:startDestination="@id/flujoFragment">

    <include app:graph="@navigation/login_navgraph" />
    <include app:graph="@navigation/empresa_navgraph" />
    <include app:graph="@navigation/main_navgraph" />
    <fragment
        android:id="@+id/flujoFragment"
        android:name="com.example.vanalaeropuerto.fragments.FlujoFragment"
        android:label="fragment_flujo"
        tools:layout="@layout/fragment_flujo" >
        <action
            android:id="@+id/action_flujoFragment_to_login_navgraph"
            app:destination="@id/login_navgraph" />
        <action
            android:id="@+id/action_flujoFragment_to_empresa_navgraph"
            app:destination="@id/empresa_navgraph" />
        <action
            android:id="@+id/action_flujoFragment_to_main_navgraph"
            app:destination="@id/main_navgraph" />
    </fragment>
</navigation>